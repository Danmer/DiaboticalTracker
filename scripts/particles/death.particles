death_skull 
 {
	 	
	map particle_sheet5.png
	region skull
	animation_speed 10
	animation_start 0
	blend add
	max_particles 1
	max_emissions 1
	lifetime .75
	fade_in 0
	fade_out 0.0
	rotation_start 90 
	
	
	period 0
	color_start 6 1 0.5 1
	color_end 6	 1 0.5 0.1

	scale_start 0	
	scale_end 130
	aspect_start 1
	aspect_end 1
	auto_orient_3d true
scale_timeline 0=0,l;0.2=0,l;0.3=1,l;0.5=1,l;1=1,l
	range 0
}
death_skull_2
 {
	 	
	map particle_sheet5.png
	region skull_8
	blend add
	max_particles 1
	max_emissions 1
	delay 0.5
	lifetime .75
	fade_in 0.2
	fade_out 0.0
	rotation_start 90 
	period 0
	color_start 6 1 0.5 0.1
	color_end 6	 1 0.5 0

	scale_start 130	
	scale_end 110
	aspect_start 1
	aspect_end 1
	auto_orient_3d true

	range 0
}



death_ring_1 
{
	 map particle_sheet5.png
	animation_speed 16
	 region explosion_ring_5

	blend add
	//animation_speed 24
	max_particles 2 
	max_emissions 2
	lifetime 0.5
	fade_out 0.0
	fade_in 1
	period 0
	delay 0.075
	turbulence 60 1
	position 0 0 1 0 0 1
	scale_start 200
	scale_end 300
	color_start 3 3 1 1
	color_end 6 0 0 0
	rotation_start 0 360
	hard_edges true
	//rotation_end 0 360
	//scale_timeline 0=0,l;0.5=0.7,l;0.8=1,l;1=1,l
	range 0
}

death_oil_splash
{
	 map oil_splash.png
	animation_speed 22
