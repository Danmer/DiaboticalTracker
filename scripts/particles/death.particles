	death_sparks3
{
	delay 0.05
	map particle_sheet5.png
	region  spark_line
	animation_speed 10
	animation_start 0
	max_particles 25
	max_emissions 25
	lifetime .5
	fade_in 0
	blend add
	fade_out 0.03
	period 0
	color_start 10 1 0 1
color_end 8 0 0 0
	position 0 0 0  
    scale_start 20
    scale_end 40
    rotation_start 0
    fade_out 0.1
	velocity_angles 0 0 0 360 360 0
	speed_start 300
	speed_end 10
	auto_orient_3d true
	aspect_start 0.5 0.7
	aspect_end 0.3 0.2
	hard_edges true
}
