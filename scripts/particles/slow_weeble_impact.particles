slow_weeble_explode_ring
{
	map particle_sheet5.png
	region explosion_ring_2
	animation_speed 30
	animation_start 0
	blend add
	max_particles 1 
	max_emissions 1
	lifetime .2
	fade_out 0.0
	fade_in 1
	period 0
	
	turbulence 60 1
	position 0 0 1 0 0 1
	scale_start 0
	scale_end 1000

		color_start 1 1 1 1
	color_end 0 0.2 0.5 0
	rotation_start 0 360
	hard_edges true
	//rotation_end 0 360
	perspective_radius 110
	//scale_timeline 0=0.0,l;0.5=0.3,l;0.7=0.8,l;1=1,l
	 displacement true 0.9 0.01 50 0.2
}
