	map particle_sheet3.png
	 region s3_rl_muzzle_2
	blend add
max_particles 60
	max_emissions 60
	// fade_in 1
	 fade_out 4
	lifetime 4 5.5
	period 0.00022	
	emitter ellipsoid 100 100 100 100 100 100
	turbulence 0 0 0
	scale_start 0 2
	scale_end 2 10
	//perspective_radius 300
	perspective_radius 3
	scale_timeline 0=0.0,l;0.24=1,l;0.44=0.5,l;0.66=1,l;1=0,l;
		
	color_start 0 1 2 1
	color_end 0 1 1 2
}


